        <star value="[*,title=Wachposten,img=http://hydra-media.cursecdn.com/wow.gamepedia.com/thumb/d/d9/Security_Guard.jpg/200px-Security_Guard.jpg?version=ad18cfde2ea3c7fb2334b0e11611edf1]" title="Wachposten" img="http://hydra-media.cursecdn.com/wow.gamepedia.com/thumb/d/d9/Security_Guard.jpg/200px-Security_Guard.jpg?version=ad18cfde2ea3c7fb2334b0e11611edf1">
          <text value=" "/>
          <wowdb value="[wowdb,uri=npc=88818]" uri="npc=88818">
            <text value="Wachposten"/>
          </wowdb>
          <text value=" wirken gelegentlich "/>
          <wowdb value="[wowdb,uri=spell=160382]" uri="spell=160382"/>
          <text value=". "/>
          <wowdb value="[wowdb,uri=spell=160382]" uri="spell=160382"/>
          <text value=" erzeugt ein Gebiet, welches den erlittenen Schaden der darin befindlichen freundlichen Einheiten um 90% reduziert."/>
        </star>
        <star value="[*,title=Gebläsearbeiter,img=http://wow.zamimg.com/uploads/screenshots/resized/467787.jpg]" title="Gebläsearbeiter" img="http://wow.zamimg.com/uploads/screenshots/resized/467787.jpg">
          <text value=" Die verbliebenen "/>
          <wowdb value="[wowdb,uri=npc=88821]" uri="npc=88821">
            <text value="Gebläsearbeiter"/>
          </wowdb>
          <text value=" müssen in Phase 2 getankt werden. Aber Achtung, sie wirken jetzt "/>
          <wowdb value="[wowdb,uri=spell=177756]" uri="spell=177756"/>
          <text value="!"/>
        </star>
        <star value="[*,title=Urelementarist,img=http://images.staticjw.com/wor/7152/primal-elementalist.png]" title="Urelementarist" img="http://images.staticjw.com/wor/7152/primal-elementalist.png">
          <text value=" In Phase 2 greifen 4 "/>
          <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
            <text value="Urelementarist"/>
          </wowdb>
          <text value="nen in den Kampf ein. Sie besitzen einen "/>
          <wowdb value="[wowdb,uri=spell=155176]" uri="spell=155176"/>
          <text value=", der sie immun gegen jeglichen Schaden macht. Dieses Schild kann durch den Effekt von "/>
          <wowdb value="[wowdb,uri=spell=176133]" uri="spell=176133"/>
          <text value=" zerstört werden, den die Schlackeelementare beim Tod wirken. Wird der Schild zerstört, erhalten die "/>
          <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
            <text value="Urelementarist"/>
          </wowdb>
          <text value="nen den Effekt "/>
          <wowdb value="[wowdb,uri=spell=158345]" uri="spell=158345"/>
          <text value=", wodurch sie 15 Sekunden lang verwundbar werden. In dieser Zeit wirken sie den Zauber "/>
          <wowdb value="[wowdb,uri=spell=155173]" uri="spell=155173"/>
          <text value=" auf sich selbst, der nach 8 Sekunden 30% ihrer Lebenspunkte wiederherstellt und von Spielern gereinigt werden muss."/>
        </star>
        <star value="[*,title=Feuerrufer,img=http://images.staticjw.com/wor/2480/firecaller.png]" title="Feuerrufer" img="http://images.staticjw.com/wor/2480/firecaller.png">
