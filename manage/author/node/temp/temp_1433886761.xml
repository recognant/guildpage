    <star value="[*,title=Phase 1]" title="Phase 1">
      <header value="[header]">
        <list value="[list,type=column]" type="column">
          <star value="[*]">
            <text value=" "/>
            <title value="[title]">
              <size value="[size,type=large]" type="large">
                <text value="Ziel: Sabotage"/>
              </size>
            </title>
            <text value="Das Ziel in Phase 1 ist die Zerstörung der beiden "/>
            <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
              <text value="Hitzeregler"/>
            </wowdb>
            <text value=", die links und rechts am Schmelzofen angebracht sind (siehe rechtes Bild). Die "/>
            <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
              <text value="Hitzeregler"/>
            </wowdb>
            <text value=" sind dabei nicht von Spielern angreifbar, können aber durch "/>
            <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
              <text value="Bombe"/>
            </wowdb>
            <text value="n von "/>
            <wowdb value="[wowdb,uri=npc=88820]" uri="npc=88820">
              <text value="Schmelzofen-Ingenieur"/>
            </wowdb>
            <text value="en Schaden erleiden. Die Aufgabe der Spieler besteht also darin, "/>
            <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
              <text value="Bombe"/>
            </wowdb>
            <text value="n zu den "/>
            <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
              <text value="Hitzeregler"/>
            </wowdb>
            <text value="n zu bringen und dann ihre "/>
            <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
              <text value="Bombe"/>
            </wowdb>
            <text value="n manuell zu detonieren. Sind beide "/>
            <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
              <text value="Hitzeregler"/>
            </wowdb>
            <text value=" zerstört, beginnt Phase 2."/>
          </star>
          <star value="[*]">
            <text value=" "/>
            <img value="[img,uri=http://wowimg.zamimg.com/uploads/guide/images/7134.jpg]" uri="http://wowimg.zamimg.com/uploads/guide/images/7134.jpg"/>
          </star>
        </list>
      </header>
      <title value="[title]">
        <size value="[size,type=large]" type="large">
          <text value="Gegnerische Einheiten"/>
        </size>
      </title>
      <list value="[list,type=media]" type="media">
        <star value="[*,img=http://www.cinemablend.com/images/sections/69474/World_Of_Warcraft__Warlords_Of_Draenor_69474.jpg,title=Schmelzofen]" img="http://www.cinemablend.com/images/sections/69474/World_Of_Warcraft__Warlords_Of_Draenor_69474.jpg" title="Schmelzofen">
          <text value=" Der Schmelzofen wirkt "/>
          <wowdb value="[wowdb,uri=spell=155209]" uri="spell=155209">
            <text value="Flammenzunge"/>
          </wowdb>
          <text value=" in regelmäßigen Abständen (basierend auf seiner Hitze), wodurch allen Spielern Schaden zu gefügt wird."/>
          <list value="[list]">
            <star value="[*]">
              <text value=" 0-24 Hitze: alle 30 Sekunden."/>
            </star>
            <star value="[*]">
              <text value=" 25-49 Hitze: alle 20 Sekunden."/>
            </star>
            <star value="[*]">
              <text value=" 50-74 Hitze: alle 15 Sekunden."/>
            </star>
            <star value="[*]">
              <text value=" 75-99 Hitze: alle 12 Sekunden."/>
            </star>
            <star value="[*]">
              <text value=" 100 Hitze: alle 10 Sekunden."/>
            </star>
          </list>
        </star>
        <star value="[*,img=http://wow.zamimg.com/uploads/screenshots/thumb/456345.jpg,title=Hitzeregler]" img="http://wow.zamimg.com/uploads/screenshots/thumb/456345.jpg" title="Hitzeregler">
          <text value=" Am Schmelzofen sind zwei "/>
          <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
            <text value="Hitzeregler"/>
          </wowdb>
          <text value=" angebracht. Das Ziel in Phase 1 besteht darin, diese zu zerstören und damit in Phase 2 zu gelanden. Der "/>
          <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
            <text value="Hitzeregler"/>
          </wowdb>
          <text value=" hat 1.191.608 Lebenspunkte, wodurch es 12 "/>
          <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
            <text value="Bombe"/>
          </wowdb>
          <text value="n bedarf, um den "/>
          <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
            <text value="Hitzeregler"/>
          </wowdb>
          <text value=" zu sprengen."/>
          <alert value="[alert,type=danger]" type="danger">
            <wowdb value="[wowdb,uri=npc=88820]" uri="npc=88820">
              <text value="Schmelzofen-Ingenieur"/>
            </wowdb>
            <text value="e reparieren den "/>
            <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
              <text value="Hitzeregler"/>
            </wowdb>
            <text value=" gelegentlich und heilen ihn damit um 3% seiner maximalen Lebenspunkte!"/>
          </alert>
        </star>
        <star value="[*,img=http://hydra-media.cursecdn.com/wow.gamepedia.com/thumb/c/c6/Furnace_Engineer.jpg/200px-Furnace_Engineer.jpg?version=1497a275a87ef4fdbce5aa090a1e3d90,title=Schmelzofeningenieur]" img="http://hydra-media.cursecdn.com/wow.gamepedia.com/thumb/c/c6/Furnace_Engineer.jpg/200px-Furnace_Engineer.jpg?version=1497a275a87ef4fdbce5aa090a1e3d90" title="Schmelzofeningenieur">
          <text value=" Gelegentlich greifen "/>
          <wowdb value="[wowdb,uri=npc=88820]" uri="npc=88820">
            <text value="Schmelzofeningenieur"/>
          </wowdb>
          <text value="e in das Kampfgeschehen ein. Sie tragen 5 "/>
          <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
            <text value="Bombe"/>
          </wowdb>
          <text value="n bei sich, die sie auf zufällige nahe Spieler werfen. Die "/>
          <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
            <text value="Bombe"/>
          </wowdb>
          <text value="n explodieren nach 15 Sekunden explodieren. Der Spieler kann die "/>
          <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
            <text value="Bombe"/>
          </wowdb>
          <text value=" aber auch manuell (per "/>
          <i value="[i]">
            <text value="Extra Action Button"/>
          </i>
          <text value=") detonieren. Die Explosion verursacht dabei auch Schaden an den "/>
          <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
            <text value="Hitzeregler"/>
          </wowdb>
          <text value="n. Beim Tod wirken "/>
          <wowdb value="[wowdb,uri=npc=88820]" uri="npc=88820">
            <text value="Schmelzofen-Ingenieur"/>
          </wowdb>
          <text value="e "/>
          <wowdb value="[wowdb,uri=spell=174726]" uri="spell=174726"/>
          <text value=" und lassen einen "/>
          <wowdb value="[wowdb,uri=npc=87339]" uri="npc=87339">
            <text value="Bombensack"/>
          </wowdb>
          <text value=" fallen, der die restlichen nicht-geworfenen "/>
          <wowdb value="[wowdb,uri=spell=155192]" uri="spell=155192">
            <text value="Bombe"/>
          </wowdb>
          <text value="n enthält."/>
          <wowdb value="[wowdb,uri=npc=88820]" uri="npc=88820">
            <text value="Schmelzofen-Ingenieur"/>
          </wowdb>
          <text value="e wirken außerdem "/>
          <wowdb value="[wowdb,uri=spell=155179]" uri="spell=155179">
            <text value="reparieren"/>
          </wowdb>
          <text value=", um 3% der maximalen Lebenspunkte der "/>
          <wowdb value="[wowdb,uri=npc=76808]" uri="npc=76808">
            <text value="Hitzeregler"/>
          </wowdb>
          <text value=" wiederherzustellen."/>
          <text value="Des Weiteren verursachen sie mäßigen Schaden durch die Fähigkeit "/>
          <wowdb value="[wowdb,uri=spell=155201]" uri="spell=155201"/>
          <text value=". Um Schaden zu vermeiden, sollten Spieler 5 Meter auseinander stehen."/>
        </star>
        <star value="[*,title=Wachposten,img=http://hydra-media.cursecdn.com/wow.gamepedia.com/thumb/d/d9/Security_Guard.jpg/200px-Security_Guard.jpg?version=ad18cfde2ea3c7fb2334b0e11611edf1]" title="Wachposten" img="http://hydra-media.cursecdn.com/wow.gamepedia.com/thumb/d/d9/Security_Guard.jpg/200px-Security_Guard.jpg?version=ad18cfde2ea3c7fb2334b0e11611edf1">
          <text value=" "/>
          <wowdb value="[wowdb,uri=npc=88818]" uri="npc=88818">
            <text value="Wachposten"/>
          </wowdb>
          <text value=" wirken gelegentlich "/>
          <wowdb value="[wowdb,uri=spell=160382]" uri="spell=160382"/>
          <text value=". "/>
          <wowdb value="[wowdb,uri=spell=160382]" uri="spell=160382"/>
          <text value=" erzeugt ein Gebiet, welches den erlittenen Schaden der darin befindlichen freundlichen Einheiten um 90% reduziert. Dieser Effekt betrifft in Phase 1 keine "/>
          <wowdb value="[wowdb,uri=npc=88821]" uri="npc=88821">
            <text value="Gebläsearbeiter"/>
          </wowdb>
          <text value="."/>
        </star>
        <star value="[*,title=Gebläsearbeiter,img=http://wow.zamimg.com/uploads/screenshots/resized/467787.jpg]" title="Gebläsearbeiter" img="http://wow.zamimg.com/uploads/screenshots/resized/467787.jpg">
          <text value=" "/>
          <wowdb value="[wowdb,uri=npc=88821]" uri="npc=88821">
            <text value="Gebläsearbeiter"/>
          </wowdb>
          <text value=" erhöhen die Hitze des Ofen um 2 alle 8 Sekunden. In Phase 1 müssen die "/>
          <wowdb value="[wowdb,uri=npc=88821]" uri="npc=88821">
            <text value="Gebläsearbeiter"/>
          </wowdb>
          <text value=" nicht getankt werden."/>
        </star>
        <star value="[*,title=Vorarbeiter Feldspat,img=http://wow.zamimg.com/uploads/screenshots/resized/456461.jpg]" title="Vorarbeiter Feldspat" img="http://wow.zamimg.com/uploads/screenshots/resized/456461.jpg">
          <text value=" Dieser Mini-Boss ist von Anfang an aktiv und muss getankt werden. "/>
          <wowdb value="[wowdb,uri=npc=76809]" uri="npc=76809">
            <text value="Vorarbeiter Feldspat"/>
          </wowdb>
          <text value=" wirkt folgende Fähigkeiten:"/>
          <list value="[list,type=row]" type="row">
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=156937]" uri="spell=156937"/>
              <text value=" wird auf einen zufälligen Spieler gewirkt und kann unterbrochen werden."/>
            </star>
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=156934]" uri="spell=156934"/>
              <text value=" muss an der Seite abgelegt werden."/>
            </star>
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=158247]" uri="spell=158247"/>
              <text value=" betrifft nahe Spieler (~20-Meter-Radius) und muss schlichtweg geheilt werden."/>
            </star>
          </list>
        </star>
      </list>
      <alert value="[alert,type=danger]" type="danger">
        <b value="[b]">
          <text value="Kill-Priorität:"/>
        </b>
