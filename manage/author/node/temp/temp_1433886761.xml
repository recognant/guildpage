        <text value=" &gt; "/>
        <wowdb value="[wowdb,uri=npc=88818]" uri="npc=88818">
          <text value="Wachposten"/>
        </wowdb>
      </alert>
    </star>
    <star value="[*,title=Phase 2]" title="Phase 2">
      <header value="[header]">
        <list value="[list,type=column]" type="column">
          <star value="[*]">
            <text value=" "/>
            <title value="[title]">
              <size value="[size,type=large]" type="large">
                <text value="Ziel: Das Herz freilassen"/>
              </size>
            </title>
            <text value="Das Ziel in Phase 2 ist das Freilassen des Herz des Berges. Dies gelingt nur, wenn man alle 4 "/>
            <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
              <text value="Urelementarist"/>
            </wowdb>
            <text value="nen tötet. Allerdings muss zuvor ihr "/>
            <wowdb value="[wowdb,uri=spell=155176]" uri="spell=155176"/>
            <text value=" entfernt werden, indem man ein "/>
            <wowdb value="[wowdb,uri=npc=78463]" uri="npc=78463">
              <text value="Schlackeelementar"/>
            </wowdb>
            <text value=" neben einer "/>
            <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
              <text value="Urelementarist"/>
            </wowdb>
            <text value=" auf 1 Lebenspunkt bringt. Dies löst eine Explosion des "/>
            <wowdb value="[wowdb,uri=npc=78463]" uri="npc=78463">
              <text value="Schlackeelementar"/>
            </wowdb>
            <text value="s aus, die den Schild entfernt und die "/>
            <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
              <text value="Urelementarist"/>
            </wowdb>
            <text value=" mit "/>
            <wowdb value="[wowdb,uri=spell=158345]" uri="spell=158345"/>
            <text value=" belegt, wodurch sie verwundbar werden. Sind alle 4 "/>
            <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
              <text value="Urelementarist"/>
            </wowdb>
            <text value="nen getötet worden, beginnt Phase 3."/>
          </star>
          <star value="[*]">
            <text value=" "/>
            <img value="[img,uri=http://wowimg.zamimg.com/uploads/guide/images/7142.jpg]" uri="http://wowimg.zamimg.com/uploads/guide/images/7142.jpg"/>
          </star>
        </list>
      </header>
      <list value="[list,type=media]" type="media">
        <star value="[*,img=http://www.cinemablend.com/images/sections/69474/World_Of_Warcraft__Warlords_Of_Draenor_69474.jpg,title=Schmelzofen]" img="http://www.cinemablend.com/images/sections/69474/World_Of_Warcraft__Warlords_Of_Draenor_69474.jpg" title="Schmelzofen">
