          <wowdb value="[wowdb,uri=npc=76821]" uri="npc=76821">
            <text value="Feuerrufer"/>
          </wowdb>
          <text value=" müssen eingesammelt und getankt werden. Sie wirken:"/>
          <list value="[list]">
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=159408]" uri="spell=159408"/>
              <text value=" ("/>
              <icon value="[icon,type=bolt]" type="bolt"/>
              <text value="unterbrechbar"/>
              <text value=")"/>
            </star>
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=155186]" uri="spell=155186"/>
              <text value=" ("/>
              <icon value="[icon,type=bolt]" type="bolt"/>
              <text value="unterbrechbar"/>
              <text value=")"/>
            </star>
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=176121]" uri="spell=176121"/>
            </star>
            <star value="[*]">
              <text value=" "/>
              <wowdb value="[wowdb,uri=spell=159115]" uri="spell=159115"/>
              <text value=" sollte durch betäuben oder stoßen abgebrochen werden."/>
            </star>
          </list>
        </star>
        <star value="[*,title=Schlackeelementar,img=https://wow.zamimg.com/uploads/blog/thumbs/5124.png]" title="Schlackeelementar" img="https://wow.zamimg.com/uploads/blog/thumbs/5124.png">
          <text value=" Die "/>
          <wowdb value="[wowdb,uri=npc=78463]" uri="npc=78463">
            <text value="Schlackeelementar"/>
          </wowdb>
          <text value="e "/>
          <wowdb value="[wowdb,uri=spell=155196]" uri="spell=155196"/>
          <text value=" zufällige Spieler und wirken "/>
          <wowdb value="[wowdb,uri=spell=155200]" uri="spell=155200"/>
          <text value=" ("/>
          <icon value="[icon,type=bolt]" type="bolt"/>
          <text value="unterbrechbar"/>
          <text value=") auf sie. Diese Fähigkeit muss unterbrochen werden und das "/>
          <wowdb value="[wowdb,uri=npc=78463]" uri="npc=78463">
            <text value="Schlackeelementar"/>
          </wowdb>
          <text value=" zu einem "/>
          <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
            <text value="Urelementarist"/>
          </wowdb>
          <text value="en gebracht und dort auf 1 Lebenspunkt gebracht werden. Erreicht ein "/>
          <wowdb value="[wowdb,uri=npc=78463]" uri="npc=78463">
            <text value="Schlackeelementar"/>
          </wowdb>
          <text value=" 1 Lebenspunkt, explodiert es und entfernt dabei den "/>
          <wowdb value="[wowdb,uri=spell=155176]" uri="spell=155176"/>
          <text value=" der "/>
          <wowdb value="[wowdb,uri=npc=76815]" uri="npc=76815">
            <text value="Urelementarist"/>
          </wowdb>
          <text value="nen."/>
        </star>
      </list>
    </star>
    <star value="[*,title=Phase 3]" title="Phase 3">
